sno=int(input("enter student number"))
sname=input("enter student name")
group=input("enter group name")
s1=int(input("enter Os marks"))
s2=int(input("enter ds marks"))
s3=int(input("enter python marks"))
total=s1+s2+s3
avg=total//3
if avg>=90:
print("O-Grade")
elif avg>=80:
print("A=Grade")
elif avg>=70:
print5("B-Grade")
elif avg>=60:
print("C-Grade")
elif avg>=50:
print("D-Grade")
else:
print("pass")
